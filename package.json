{
  "name": "letsrateit-db",
  "version": "0.0.1",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start-server": "tsc",
    "start": "nodemon -e ts -w ./ -x npm run start-server",
    "kill": "kill $(lsof -t -i:3000)",
    "reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run migrate && npm run seed",
    "remigrate": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "moment": "^2.29.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/node": "^16.3.3",
    "@types/sequelize": "^4.28.10"
  }
}
