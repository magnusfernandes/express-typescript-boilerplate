  "main": "server.ts",
  "scripts": {
    "start-server": "tsc && node build/server.js",
    "start": "nodemon -e ts -w ./ -x npm run start-server",
    "kill": "kill $(lsof -t -i:3000)",
    "reset": "sequelize db:drop && sequelize db:create && npm run migrate && npm run seed",
    "remigrate": "sequelize db:migrate:undo:all && npm run migrate && npm run seed",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all"
  },
